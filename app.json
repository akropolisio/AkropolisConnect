{
  "expo": {
    "name": "akropolisconnect",
    "slug": "akropolisconnect",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.testcompany.testname",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "trust-rn-example"
          },
          "category": [
            "DEFAULT"
          ]
        }
      ]
    }
  }
}